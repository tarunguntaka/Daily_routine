   "source": [
    "#assuming the array is sorted in desc order\n",
    "def b_search(arr,q):\n",
    "    low,high = 0 , len(arr)-1\n",
    "    while(low<=high):\n",
    "        mid = (low+high)//2\n",
    "        mid_v = arr[mid]\n",
    "        \n",
    "        if q > mid_v:\n",
    "            high = mid-1\n",
    "        elif q < mid_v:\n",
    "            low = mid+1\n",
    "        elif q == mid_v:\n",
    "            return mid\n",
    "    return -1\n",
    "        "
   ]
